<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	
	<package name="struts2-schedule" extends="struts2-spring3">
		<action name="JobDetailAction_*" method="{1}" class="cn.log.app.schedule.action.JobDetailAction">
			<result name="success" type="redirect">log/index.jsp</result>
			<result name="error" type="redirect">login/login.jsp</result>
			<result name="logout" type="redirect">login/login.jsp</result>
		</action>
	</package>
	
</struts>
