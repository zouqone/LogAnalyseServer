<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">
    
<struts>
	<package name="struts2-UserVo" extends="struts2-spring3">
		<action name="UserVoAction_*" method="{1}" class="cn.log.app.user.action.UserVoAction">
			<result name="list" >app/jsp/userVo/userVoList.jsp</result>
			<result name="insert" >app/jsp/userVo/userVoInsert.jsp</result>
			<result name="update">app/jsp/userVo/userVoInsert.jsp</result>
			<result name="detail" >app/jsp/userVo/userVoDetail.jsp</result>
			<result name="error" ></result>
			
		</action>
	</package>
	
</struts>
