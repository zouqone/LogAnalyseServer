<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	
	<package name="struts2-report" extends="struts2-spring3">
		<action name="ReportAction_*" method="{1}" class="cn.log.app.report.web.ReportAction">
			<result name="success" type="redirect">log/index.jsp</result>
			<result name="example" type="redirect">app/report/ireport.jsp</result>
			<result name="logout" type="redirect">login/login.jsp</result>
		</action>
		<action name="px" >
			<result type="redirect" >images/report/px.gif</result>
		</action>
	</package>
	
</struts>
